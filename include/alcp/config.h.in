// config.h is cmake generated, please modify config.h.in instead.
#ifndef _INCLUDE_CONFIG_H
#define _INCLUDE_CONFIG_H 2

#cmakedefine ALCP_CONFIG_LITTLE_ENDIAN

// CPU Identification
#cmakedefine ALCP_ENABLE_AOCL_UTILS

// ALCP Release Version
#cmakedefine AOCL_RELEASE_VERSION "@AOCL_RELEASE_VERSION@"
#cmakedefine ALCP_RELEASE_VERSION_STRING "@ALCP_RELEASE_VERSION_STRING@"

// ALCP Build environment
#cmakedefine ALCP_BUILD_ENV "@ALCP_BUILD_ENV@"

// ALCP lib path
#cmakedefine ALCP_LIB_OUTPUT_FILE_NAME_STRING                                  \
    "@ALCP_LIB_OUTPUT_FILE_NAME_STRING@"

// Compiler Detection
#cmakedefine COMPILER_IS_CLANG
#cmakedefine COMPILER_IS_GCC
#cmakedefine COMPILER_IS_MSVC

// OS Identification
#cmakedefine ALCP_BUILD_OS_LINUX
#cmakedefine ALCP_BUILD_OS_WINDOWS

// OpenSSL Overrides
#cmakedefine01 ALCP_BIGNUM_USE_OPENSSL

// CPU Identification
#cmakedefine01 ALCP_DISABLE_ASSEMBLY

#cmakedefine ALCP_ENABLE_DEBUG_LOGGING

#endif /* _INCLUDE_CONFIG_H */